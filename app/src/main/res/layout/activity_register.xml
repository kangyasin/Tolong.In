<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegisterActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/walpaper4" />

    <EditText
        android:id="@+id/editTextFullname"
        android:layout_width="250dp"
        android:layout_height="40dp"

        android:layout_alignParentEnd="true"
        android:layout_below="@+id/divider4"
        android:layout_marginEnd="55dp"
        android:background="#9c000000"
        android:ems="10"
        android:hint="  Fullname"
        android:inputType="textPersonName"
        android:textColor="#c1ffffff"
        android:textColorHint="#95ffffff" />

    <EditText
        android:id="@+id/editTextPassword"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/editTextFullname"
        android:layout_alignTop="@+id/password"
        android:background="#9c000000"
        android:ems="10"
        android:hint="  Password"
        android:inputType="textPassword"
        android:textColor="#c1ffffff"
        android:textColorHint="#95ffffff" />

    <EditText
        android:id="@+id/editTextEmail"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/editTextFullname"
        android:layout_alignTop="@+id/email"
        android:background="#9c000000"
        android:ems="10"
        android:hint="  Email"
        android:inputType="textEmailAddress"
        android:textColor="#c1ffffff"
        android:textColorHint="#95ffffff" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="250dp"
        android:layout_height="40dp"

        android:layout_alignStart="@+id/editTextFullname"
        android:layout_below="@+id/divider5"
        android:background="#9c000000"
        android:ems="10"
        android:hint="  Phone"
        android:inputType="phone"
        android:textColor="#c1ffffff"
        android:textColorHint="#95ffffff" />

    <Button
        android:id="@+id/buttonRegister"
        android:layout_width="310dp"
        android:layout_height="wrap_content"

        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="14dp"
        android:background="#763194e6"
        android:text="Submit"
        android:textColor="@android:color/background_light" />

    <Spinner
        android:id="@+id/spinnerahli1"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/editTextFullname"
        android:layout_below="@+id/divider3"
        android:background="#c6ffffff"
        android:entries="@array/Spinner1"
        android:text="Submit" />

    <Spinner
        android:id="@+id/spinnerahli2"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/editTextFullname"
        android:layout_below="@+id/divider6"
        android:background="#b8ffffff" />
    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/skill"
        android:layout_alignTop="@+id/editTextPassword"
        app:srcCompat="@android:color/black" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/spinnerahli2"
        android:layout_toStartOf="@+id/editTextFullname"
        app:srcCompat="@android:color/black" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/spinnerahli1"
        android:layout_toStartOf="@+id/editTextFullname"
        app:srcCompat="@android:color/black" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/email"
        android:layout_toStartOf="@+id/editTextFullname"
        app:srcCompat="@android:color/black" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/editTextFullname"
        android:layout_toStartOf="@+id/editTextFullname"
        app:srcCompat="@android:color/black" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/editTextPhone"
        android:layout_toStartOf="@+id/editTextFullname"
        app:srcCompat="@android:color/black" />

    <ImageView
        android:id="@+id/email"
        android:layout_width="27dp"
        android:layout_height="40dp"
        android:layout_above="@+id/divider2"
        android:layout_alignStart="@+id/skill"
        android:layout_marginStart="11dp"
        app:srcCompat="@drawable/email" />

    <ImageView
        android:id="@+id/password"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_above="@+id/divider4"
        android:layout_toStartOf="@+id/editTextFullname"
        app:srcCompat="@drawable/password" />

    <ImageView
        android:id="@+id/fullname"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/editTextFullname"
        android:layout_toStartOf="@+id/editTextFullname"
        app:srcCompat="@drawable/name" />

    <ImageView
        android:id="@+id/skill"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/spinnerahli2"
        android:layout_toStartOf="@+id/editTextFullname"
        app:srcCompat="@drawable/skill" />

    <ImageView
        android:id="@+id/category"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/spinnerahli1"
        android:layout_toStartOf="@+id/editTextFullname"
        app:srcCompat="@drawable/category" />

    <ImageView
        android:id="@+id/phone"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/editTextPhone"
        android:layout_toStartOf="@+id/editTextFullname"
        app:srcCompat="@drawable/phone" />

    <View
        android:id="@+id/divider2"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_above="@+id/editTextPassword"
        android:layout_alignParentStart="true"
        android:background="?android:attr/listDivider"
        android:visibility="invisible" />

    <View
        android:id="@+id/divider4"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:background="?android:attr/listDivider"
        android:visibility="invisible" />

    <View
        android:id="@+id/divider5"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editTextFullname"
        android:background="?android:attr/listDivider"
        android:visibility="invisible" />

    <View
        android:id="@+id/divider3"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editTextPhone"
        android:background="?android:attr/listDivider"
        android:visibility="invisible" />

    <View
        android:id="@+id/divider6"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/spinnerahli1"
        android:background="?android:attr/listDivider"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="47dp"
        app:srcCompat="@drawable/registericon" />

</RelativeLayout>