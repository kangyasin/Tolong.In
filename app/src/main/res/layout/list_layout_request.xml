<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff"

    android:orientation="vertical">

    <TextView
        android:id="@+id/textViewUserID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignTop="@+id/imageView11"
        android:layout_toEndOf="@+id/imageView13"
        android:text="Address"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textSize="10sp"
        android:textStyle="bold"
        android:visibility="visible" />

    <TextView
        android:id="@+id/textViewRequestName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_above="@+id/divider11"
        android:layout_toEndOf="@+id/imageView16"
        android:text="Name"
        android:textStyle="normal|bold|italic"
        android:visibility="visible" />

    <TextView
        android:id="@+id/textViewRequestKeahlian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignBottom="@+id/imageView11"
        android:layout_centerHorizontal="true"
        android:text="Keahlian" />

    <TextView
        android:id="@+id/textViewRequestEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/divider13"
        android:layout_toEndOf="@+id/imageView12"
        android:text="Email"
        android:textSize="13sp"
        android:textStyle="italic" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="18dp"
        app:srcCompat="@drawable/iconrequest" />

    <View
        android:id="@+id/divider11"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_above="@+id/textViewRequestKeahlian"
        android:layout_alignParentStart="true"
        android:background="?android:attr/listDivider"
        android:visibility="invisible" />

    <View
        android:id="@+id/divider13"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_above="@+id/textViewRequestName"
        android:layout_alignParentStart="true"
        android:background="?android:attr/listDivider"
        android:visibility="invisible" />

    <View
        android:id="@+id/divider15"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_above="@+id/textViewRequestEmail"
        android:layout_alignParentStart="true"
        android:background="?android:attr/listDivider"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="19dp"
        android:layout_height="15dp"
        android:layout_alignStart="@+id/imageView15"
        android:layout_alignTop="@+id/imageView11"
        app:srcCompat="@drawable/maps" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_alignStart="@+id/imageView12"
        android:layout_alignTop="@+id/textViewRequestEmail"
        app:srcCompat="@drawable/adminemail" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_above="@+id/divider11"
        android:layout_alignStart="@+id/imageView16"
        app:srcCompat="@drawable/adminstatus" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_toStartOf="@+id/textViewRequestKeahlian"
        app:srcCompat="@drawable/adminskill" />

    <Space
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="138dp" />

</RelativeLayout>
